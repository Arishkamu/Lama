fun f (l) {
  infix === at == (a, b) {
    return a == b
  }

  case l of
    {}         -> return 1
  | {_}        -> return 1
  | a : b : tl -> return a === b && f (b : tl)
  esac
}

local x = read ();

write (f ({}));
